    "%matplotlib notebook\n",
    "\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "#definimos constantes\n",
    "omega = 2*np.pi*0.1 #rad/seg\n",
    "n = np.arange(0,128) #eje de tiempos\n",
    "\n",
    "A = np.random.randint(1,7,(10,1))\n",
    "\n",
    "x = A * np.sin(omega*n)\n",
    "\n",
    "plt.plot(x.T)"
